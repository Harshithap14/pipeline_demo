pipeline{
    agent any
            stages{
                stage('One'){
                    steps{
                        echo 'Hi,This is Harshitha'
                    }
                }
                stage('Two'){
                    steps{
                        input('Do you want to proceed?')
                    }
                }
                stage('Three'){
                    when{
                        not{
                            branch"master
                        }
                    }
                    steps{
                        echo "Hello"
                    }
                }
                stage('Four'){
                    parallel{
                        stage('Unit test'){
                            steps{
                                echo 'running the unit test.'
                            }
                        }
                        stage('integration test'){
                            agent{
                                docker{
                                    reusenode false
                                    image 'ubuntu'
                                }
                            }
                            steps{
                                echo 'running integration test'
                            }
                        }
                    }
                }            }
    }
}
