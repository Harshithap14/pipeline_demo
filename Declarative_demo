pipeline{
    agent any
            stages{
                stage('One'){
                    steps{
                        echo 'Hi,This is Harshitha'
                    }
                }
                stage('Two'){
                    steps{
                        input('Do you want to proceed?')
                    }
                }
                stage('Three'){
                    when{
                        not{
                            branch"master"
                        }
                    }
                    steps{
                        echo "Hello"
                    }
                }
                stage('Four'){
                    parallel{
                        stage('Unit test'){
                            steps{
                                echo 'running the unit test.'
                            }
                        }
                        stage('Integration test'){
                            agent{
                                label 'docker'{
                                    reuseNode false
                                    image 'ubuntu'
                                }
                            }
                            steps{
                                echo 'Running integration test.......'
                            }
                        }
                    }
                }            }
    }

